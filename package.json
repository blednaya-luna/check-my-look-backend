{
  "name": "check-my-look-back",
  "version": "0.2.0",
  "author": "Blednaya Luna",
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "node dist/app.js",
    "server-debug": "nodemon --inspect dist/app.js",
    "debug": "npm run build && npm run watch-debug",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run server-debug\"",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "test": "jest --forceExit --coverage --verbose --passWithNoTests",
    "watch-test": "npm run test -- --watchAll"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-status-monitor": "^1.2.8",
    "express-validator": "^6.3.1",
    "lusca": "^1.6.1",
    "mongoose": "^5.8.7",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-instagram": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/connect-mongo": "^3.0.0",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/express-status-monitor": "^1.2.0",
    "@types/jest": "^24.0.25",
    "@types/lusca": "^1.6.1",
    "@types/mongoose": "^5.5.41",
    "@types/morgan": "^1.7.37",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/passport-twitter": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=12.14.1",
    "yarn": ">=1.21.1"
  }
}
